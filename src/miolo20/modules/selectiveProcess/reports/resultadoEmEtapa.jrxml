	<queryString>
		<![CDATA[SELECT SP.description AS processoseletivo,
           STP.description AS etapa
      FROM spr.step STP
INNER JOIN spr.selectiveProcess SP
        ON SP.selectiveProcessId = STP.selectiveProcessId
     WHERE STP.stepId = $P{stepid}]]>
	</queryString>
