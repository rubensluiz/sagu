class sAcaoEmMassa
{
    /**
     * Label exibicao
     *
     * @var string
     */
    private $label;
    
    /**
     * Metodo que sera chamado no form.
     *
     * @var string
     */
    private $metodo;
    
    /**
     * Icone exibicao (opcional)
     * 
     * @var string
     */
    private $icone;
    
    /**
     * Flag que indica se deve ser pedido confirmacao ao usuario quando clicar no botao.
     *
     * @var boolean
     */
    private $pedirConfirmacao = false;
    
    /**
     * Flag que indica se deve executar o metodo e retornar para busca.
     *
     * @var boolean
     */
    private $retornarParaBusca = false;
    
    public function getLabel()
    {
        return $this->label;
    }

    public function setLabel($label)
    {
        $this->label = $label;
        
        return $this;
    }

    public function getMetodo()
    {
        return $this->metodo;
    }

    public function setMetodo($metodo)
    {
        $this->metodo = $metodo;
        
        return $this;
    }

    public function getIcone()
    {
        return $this->icone;
    }

    public function setIcone($icone)
    {
        $this->icone = $icone;
        
        return $this;
    }

    public function getPedirConfirmacao()
    {
        return $this->pedirConfirmacao;
    }

    public function setPedirConfirmacao($pedirConfirmacao)
    {
        $this->pedirConfirmacao = $pedirConfirmacao;
        
        return $this;
    }

    public function getRetornarParaBusca()
    {
        return $this->retornarParaBusca;
    }

    public function setRetornarParaBusca($retornarParaBusca)
    {
        $this->retornarParaBusca = $retornarParaBusca;
        
        return $this;
    }
}
