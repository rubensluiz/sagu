<?php
class sThreadTest extends sThread
{
    private $nome;
    
    protected function run()
    {
        for ($i=0; $i < 100000000; $i++)
        {
            if ( ($i % 100000) == 0 )
            {
                file_put_contents("/tmp/thread_" . $this->getNome(), $i . ' __' . rand());
            }
        }
    }
    
    public function getNome()
    {
        return $this->nome;
    }

    public function setNome($nome)
    {
        $this->nome = $nome;
    }
}
?>
