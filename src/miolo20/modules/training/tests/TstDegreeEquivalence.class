require_once('../../basic/classes/SUnitTest.class');

class TstDegreeEquivalence extends SUnitTest
{
    private static $id;
    
    protected function setUp()
    {
        parent::setUp();

        $this->pkeys = array( 'degreeEquivalenceId' );

        $this->addType(self::getObj());
    }
    
    public static function getObj()
    {
        $type = new TraDegreeEquivalence();
        $type->description = 'Equivalencia 1';
        $type->beginDate = SAGU::getDateNow();
        $type->endDate = SAGU::addIntervalInDate($type->beginDate, 'd', 10);
        
        return $type;
    }
    
    public static function getId()
    {
        if ( strlen(self::$id) <= 0 )
        {
            $obj = self::getObj();
            $obj->save();
            
            self::$id = $obj->degreeEquivalenceId;
        }
        
        return self::$id;
    }
}

