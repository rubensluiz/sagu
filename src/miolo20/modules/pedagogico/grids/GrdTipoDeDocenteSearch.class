<?php

class GrdTipoDeDocenteSearch extends SGrid
{
    public function __construct($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $module = MIOLO::getCurrentModule();

        $columns = array(
            'acptipodedocente.tipodedocenteid' => new sGridColumn( null, _M('Código', $module), 'right'),
            'acptipodedocente.descricao' => new sGridColumn( null, _M('Descrição', $module), 'left'),
        );
        
        $primaryKey = array('tipodedocenteid' => '%acptipodedocente.tipodedocenteid%');
        
        parent::__construct($filters, $columns, __CLASS__, $primaryKey);
    }
}