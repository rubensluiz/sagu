class GrdStageDetailsStudentSelect extends MGrid
{
    public function __construct($filters)
    {
        $MIOLO = MIOLO::getInstance();
        $module = SAGU::getFileModule(__FILE__);

        $columns = array(
            new MGridColumn( _M('Código', $module), 'right', true, null, true, null, false ),
            new MGridColumn( _M('Nome', $module), 'left',  true, null, true, null, false ),
        );
        
        parent::__construct($filters, $columns, __CLASS__);

        $this->addActionSelect();   
        $this->setPageLength(99999);
    }
    
    function generateNavigationHeader()
    {
        return null;
    }
    
    function generateNavigationFooter()
    {
        return null;
    }
}
