	<subDataset name="historico">
		<parameter name="contractid" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT  EXTRACT( YEAR FROM E.beginDate ) as year,
                        E.curricularComponentId,
                        E.name,
                        E.academicNumberhours,
                        B.numberHourExploitation,
                        ( SELECT I.description
                            FROM acdEnrollStatus I
                           WHERE B.statusId = I.statusId ) as status,
                       CASE WHEN C.useConcept
                       THEN (    SELECT COALESCE( O.concept, '')::TEXT
                                   FROM acdDegreeEnroll O
                             INNER JOIN acdDegree P
                                        ON(     O.degreeId = P.degreeId
                                            AND P.parentDegreeId IS NULL
                                            AND O.enrollId = B.enrollId )
                             )
                       ELSE (    SELECT ROUND( O.note::NUMERIC, getParameter('BASIC', 'GRADE_ROUND_VALUE')::int)::TEXT
                                   FROM acdDegreeEnroll O
                             INNER JOIN acdDegree P
                                        ON(     O.degreeId = P.degreeId
                                            AND P.parentDegreeId IS NULL
                                            AND O.enrollId = B.enrollId )
		                    ORDER BY O.recorddate DESC LIMIT 1
                            )
                        END,
                        E.academicCredits,
(SELECT array_to_string(array_agg(T.description||' '||S.name),',') FROM acdSchedule Q INNER JOIN acdscheduleprofessor R ON Q.scheduleId = R.scheduleId INNER JOIN basPhysicalPersonProfessor S ON R.professorid = S.personid LEFT JOIN baspersontitle T ON T.persontitleid = S.persontitleid WHERE Q.groupId = C.groupId ) as professores
                  FROM unit_acdContract A
             LEFT JOIN unit_acdEnroll B
                       ON( A.contractId = B.contractId )
             LEFT JOIN unit_acdGroup C
                       ON( B.groupId = C.groupId )
             LEFT JOIN unit_acdcurriculum D
                       ON(     C.curriculumId = D.curriculumId
                           AND D.courseId = A.courseId
                           AND D.courseVersion = A.courseVersion
                           AND D.turnId = A.turnId
                           AND D.unitId = A.unitId
                           AND D.showInScholarshipDescription )
             LEFT JOIN acdCurricularComponent E
                       ON ( D.curricularComponentId = E.curricularComponentId )
                 WHERE A.contractId = $P{contractid}
                   AND D.curriculumTypeId NOT IN (getParameter('ACADEMIC', 'ACD_CURRICULUM_TYPE_PROFICIENCY')::int)
GROUP BY 1,2,3,4,5,6,7,8,9]]>
		</queryString>
		<field name="year" class="java.lang.Double"/>
		<field name="curricularcomponentid" class="java.lang.String"/>
		<field name="name" class="java.lang.String"/>
		<field name="academicnumberhours" class="java.lang.Double"/>
		<field name="numberhourexploitation" class="java.lang.Double"/>
		<field name="status" class="java.lang.String"/>
		<field name="case" class="java.lang.String"/>
		<field name="academiccredits" class="java.lang.Double"/>
		<field name="professores" class="java.lang.String"/>
	</subDataset>
	<parameter name="contractid" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="emissiondate" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT B.personId,
                       B.name as nome,
                       TO_CHAR(B.dateBirth, getParameter('BASIC', 'MASK_DATE')) as dateBirth,
                       D.name as stateName,
                       C.stateId,
                       F.content as rg,
                       F.organ as orgao,
                       E.nationality as nacionalidade,
                       CASE WHEN A.externalCourseId IS NOT NULL
                            THEN I.name
                            ELSE CASE WHEN A.internalContractId IS NOT NULL
                                      THEN   (SELECT G.shortName
                                                FROM unit_acdContract F
                                           LEFT JOIN acdCourse G
                                                     ON ( F.courseId = G.courseId )
                                               WHERE F.contractId = A.internalContractId)
                                      ELSE   (SELECT G.shortName
                                                FROM unit_acdcourseoccurrence F
                                           LEFT JOIN acdCourse G
                                                     ON ( F.courseId = G.courseId )
                                               WHERE A.internalCourseOccurrenceId = F.courseId
                                                 AND A.internalCourseOccurrenceVersion = F.courseVersion
                                                 AND A.internalTurnId = F.turnId
                                                 AND A.internalUnitId = F.unitId    )
                                       END
                            END as externalcourseName,
                       CASE WHEN A.externalConclusionDate IS NOT NULL
                            THEN TO_CHAR(A.externalConclusionDate, getParameter('BASIC', 'MASK_DATE'))
                            ELSE (SELECT TO_CHAR(H.conclusionDate, getParameter('BASIC', 'MASK_DATE'))
                                    FROM unit_acdContract H
                                   WHERE H.contractId = A.internalContractId)
                             END as externalconclusionDate,
                       CASE WHEN A.externalConclusionInstitutionId IS NOT NULL
                            THEN (SELECT F.shortName
                                    FROM basLegalPerson F
                                   WHERE F.personId = A.externalConclusionInstitutionId)
                            ELSE CASE WHEN     A.internalContractId IS NOT NULL
                                            OR A.internalCourseOccurrenceId IS NOT NULL
                                      THEN (SELECT F.name
                                              FROM basCompanyConf F
                                             WHERE companyId = getParameter('BASIC', 'DEFAULT_COMPANY_CONF')::INT)
                                       END
                             END as company,
                            J.shortName as shortCourseName,
		        J.name as courseName,
                            buscarPortariaVigente(A.courseId, A.courseVersion, A.turnId, A.unitId, now()::date) as documentodereconhecimento,
                            A.conclusionDate,
 		       K.hourtotal
                  FROM unit_acdContract A
        LEFT JOIN ONLY basPhysicalPerson B
                       ON (B.personId = A.personId)
             LEFT JOIN basCity C
                       ON ( B.cityIdBirth = C.cityId )
             LEFT JOIN basState D
                       ON ( C.stateId = D.stateId )
             LEFT JOIN basCountry E
                       ON ( D.countryId = E.countryId )
             LEFT JOIN basDocument F
                       ON ( A.personId = F.personId AND F.documentTypeId = getparameter('BASIC', 'DEFAULT_DOCUMENT_TYPE_ID_RG')::int )
             LEFT JOIN acdExternalCourse I
                       ON ( A.externalCourseId = I.externalCourseId )
             LEFT JOIN acdCourse J
                       ON A.courseId = J.courseId
             LEFT JOIN acdCourseVersion K
                       ON K.courseId = A.courseId AND K.courseVersion = A.courseVersion
                 WHERE A.contractId = $P{contractid}]]>
	</queryString>
