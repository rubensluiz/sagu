		</parameter>
		<queryString>
			<![CDATA[SELECT COALESCE(A.courseId || '/' || A.courseVersion || ' - ' || B.name || ' - Unidade ' || C.unitId || '-' || C.description || ' - Turno ' || D.turnId || '-' || D.description,
                'TOTAIS') AS courseId,
       A.courseVersion,
       A.unitId,
       A.turnId,
       A.queryYear,
       A.queryMonth,
       ROUND(A.availableProgrammedHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS availableProgrammedHours,
       ROUND(A.availableRealizedHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS availableRealizedHours,
       ROUND(A.programmedTeachHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS programmedTeachHours,
       ROUND(A.realizedTeachHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS realizedTeachHours,
       ROUND(A.programmedPreparationHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS programmedPreparationHours,
       ROUND(A.realizedPreparationHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS realizedPreparationHours,
       ROUND(A.directionHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS directionHours,
       ROUND(A.internalExternalActivitiesProgrammedHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS internalExternalActivitiesProgrammedHours,
       ROUND(A.internalExternalActivitiesRelizedHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS internalExternalActivitiesRelizedHours,
       ROUND(A.totalProgrammedHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS totalProgrammedHours,
       ROUND(A.totalRealizedHours::numeric,
             getParameter('BASIC', 'REAL_ROUND_VALUE')::int) AS totalRealizedHours,
       $P{course}::varchar AS course
  FROM professorTimeSheet($P{professorId},
                          $P{beginMonth},
                          $P{beginYear},
                          $P{endMonth},
                          $P{endYear},
                          $P{courseId},
                          $P{courseVersion},
                          $P{unitId},
                          $P{turnId}) A
LEFT JOIN acdCourse B
       ON B.courseId = A.courseId
LEFT JOIN basUnit C
       ON C.unitId = A.unitId
LEFT JOIN basTurn D
       ON D.turnId = A.turnId]]>
		</queryString>
