	<queryString>
		<![CDATA[SELECT DISTINCT
       (SELECT name || ' - PLANILHA DE HORAS DO PROFESSOR' FROM bascompanyconf order by companyid LIMIT 1) as titulo,
       A.name,
       B.timeSheet,
       B.workLoad,
       B.teachWorkLoad,
       C.courseId,
       C.courseVersion,
       C.unitId,
       C.turnId,
       $P{professorId}::int AS professorId,
       $P{beginMonth}::int AS beginMonth,
       $P{beginYear}::int AS beginYear,
       $P{endMonth}::int AS endMonth,
       $P{endYear}::int AS endYear,
       C.course
  FROM ONLY basPerson A,
            getProfessorCurrentCommitment($P{professorId}::int) B,
            (    SELECT NULL::varchar AS courseId,
                        NULL::integer AS courseVersion,
                        NULL::integer AS unitId,
                        NULL::integer AS turnId,
                        'TOTAIS'::varchar AS course
              UNION ALL
                 SELECT A.courseId,
                        A.courseVersion,
                        A.unitId,
                        A.turnId,
                        getCourseOccurrenceDescription(A.courseId, A.courseVersion, A.unitId, A.turnId)::varchar
                   FROM unit_acdcurriculum A
             INNER JOIN unit_acdGroup B
                     ON B.curriculumId = A.curriculumId
                  WHERE CASE WHEN $P{courseId} IS NULL THEN true ELSE A.courseid IN ($P{courseId}) END AND EXISTS (SELECT 1
                                  FROM acdSchedule X
                            INNER JOIN acdScheduleProfessor Y
                                    ON Y.scheduleId = X.scheduleId
                                 WHERE X.groupId = B.groupId
                                   AND Y.professorId = $P{professorId})) C
 WHERE A.personId = $P{professorId}::int ORDER BY C.courseid, C.courseversion DESC]]>
	</queryString>
