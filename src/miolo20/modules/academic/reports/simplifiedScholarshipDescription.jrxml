	<queryString>
		<![CDATA[SELECT A.personId,
               P.name as personName,
               DATAPOREXTENSO(NOW()::date) AS datahoje,
               A.courseId || '/' || A.courseVersion || ' - ' || C.name AS courseName,

               -- Obtem media global
               (SELECT ROUND(avg((SELECT note FROM acdDegreeEnroll INNER JOIN acdDegree USING (degreeId) WHERE enrollId = _B.enrollId AND degreenumber = 0 ORDER BY recorddate DESC LIMIT 1))::NUMERIC, GETPARAMETER('BASIC', 'GRADE_ROUND_VALUE')::int)
                  FROM unit_acdContract _A
            INNER JOIN unit_acdEnroll _B
                    ON ( _A.contractId = _B.contractId )
             LEFT JOIN unit_acdGroup _C
                    ON ( _B.groupId = _C.groupId )
             LEFT JOIN unit_acdlearningperiod _D
                    ON ( _D.learningPeriodId = _C.learningPeriodId )
                 WHERE
                  _B.statusId NOT IN(
                    GETPARAMETER('ACADEMIC', 'ENROLL_STATUS_ENROLLED')::int,
                    GETPARAMETER('ACADEMIC', 'ENROLL_STATUS_CANCELLED')::int,
                    GETPARAMETER('ACADEMIC', 'ENROLL_STATUS_DESISTING')::int,
                    GETPARAMETER('ACADEMIC', 'ENROLL_STATUS_EXCUSED')::int,
                    GETPARAMETER('ACADEMIC', 'ENROLL_STATUS_DISAPPROVED_FOR_LACKS')::int,
                    GETPARAMETER('ACADEMIC', 'ENROLL_STATUS_DISAPPROVED')::int)
                   AND _A.contractId = A.contractId) AS mediaglobal,


               TO_CHAR(A.formationDate, ( SELECT value
                                            FROM basconfig
                                           WHERE parameter = 'MASK_DATE'
                                             AND moduleconfig = 'BASIC'
                                           LIMIT 1)) AS formationDate, -- Data de Colação de Grau
               TO_CHAR(A.conclusionDate, ( SELECT value
                                             FROM basconfig
                                            WHERE parameter = 'MASK_DATE'
                                              AND moduleconfig = 'BASIC'
                                            LIMIT 1)) AS conclusionDate, -- Data de Conclusão
               TO_CHAR(A.diplomaDate, ( SELECT value
                                          FROM basconfig
                                         WHERE parameter = 'MASK_DATE'
                                           AND moduleconfig = 'BASIC'
                                         LIMIT 1)) AS diplomaDate, -- Data de Expedição do Diploma
               A.subscriptionid, -- Nº de Inscrição
               F.name || ' - ' || F.stateid AS cityBirth, -- Naturalidade
               TO_CHAR(P.dateBirth, ( SELECT value
                                        FROM basconfig
                                       WHERE parameter = 'MASK_DATE'
                                         AND moduleconfig = 'BASIC'
                                       LIMIT 1)) AS dateBirth, -- Data de Nascimento
               DOC.content || COALESCE(' - ' || DOC.organ, '') AS documentrg, -- Identidade
               G.nationality, -- Nacionalidade
               buscarportariavigente(A.courseid, A.courseVersion, A.turnId, A.unitId, now()::date) as recognitionDocumentNumber, -- Portaria de Reconhecimento
               ( SELECT description
                   FROM acdstatecontract
                  WHERE statecontractid = getcontractactivationstatecontract(A.contractid)
                  LIMIT 1) AS inscriptiontype, -- Forma de Ingresso
               IO.position AS generalposition, -- Classificação no Vestibular
               ( SELECT MIN(PF.periodId)
                   FROM unit_acdEnroll PA
             INNER JOIN unit_acdGroup PB
                     ON PB.groupId = PA.groupId
             INNER JOIN unit_acdcurriculum PC
                     ON PC.curriculumId = PA.curriculumId
             INNER JOIN acdCurricularComponent PD
                     ON PD.curricularComponentId = PC.curricularComponentId
                    AND PD.curricularComponentVersion = PC.curricularComponentVersion
             INNER JOIN acdEnrollStatus PE
                     ON PE.statusId = PA.statusId
             INNER JOIN unit_acdlearningperiod PF
                     ON PF.learningPeriodId = PB.learningPeriodId
                  WHERE PA.contractId = A.contractId) AS periodId
                   FROM unit_acdContract A
         LEFT JOIN ONLY basphysicalperson P
                     ON A.personId = P.personId
              LEFT JOIN acdCourse C
                     ON A.courseId = C.courseId
              LEFT JOIN basCity F
                     ON F.cityId = P.cityIdBirth
              LEFT JOIN basCountry G
                     ON G.countryId = P.countryIdBirth
              LEFT JOIN basdocument DOC
                     ON DOC.personid = A.personid
                    AND DOC.documenttypeid = ( SELECT value::integer
                                                 FROM basConfig
                                                WHERE moduleConfig = 'BASIC'
                                                  AND parameter = 'DEFAULT_DOCUMENT_TYPE_ID_RG'
                                                LIMIT 1)
              LEFT JOIN spr.subscription I
                     ON I.subscriptionid = A.subscriptionid
              LEFT JOIN spr.subscriptionoption IO
                     ON IO.subscriptionid = I.subscriptionid
                    AND IO.subscriptionstatusid = $P{STATUS_APROVED}::int
                  WHERE ($P{contractId} IS NULL OR A.contractId = $P{contractId}::int);]]>
	</queryString>
