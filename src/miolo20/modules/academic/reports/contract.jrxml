	<subDataset name="curricularComponents">
		<parameter name="personId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="learningPeriodId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="courseId" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="courseVersion" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT CC.curricularcomponentid, -- codigo da disciplina
                  G.classid, -- turma
                  CC.name, -- disciplina
                  CC.academicCredits, -- numero de creditos
                  round(F.value, '2') AS creditValue,
                  G.financecredits * round(F.value, '2') AS price,  -- valor
                  CASE WHEN C.parcelsNumber > 0
                       THEN C.parcelsNumber
                       ELSE LP.parcelsNumber
                  END as parcels
             FROM unit_acdlearningperiod LP
        LEFT JOIN finPrice F
               ON F.courseid = LP.courseid
              AND F.courseversion = LP.courseversion
              AND F.turnid = LP.turnid
              AND F.unitid = LP.unitid
              AND (NOW() BETWEEN F.startdate AND F.enddate)
        LEFT JOIN unit_acdContract C
               ON LP.courseId = C.courseId
              AND LP.courseVersion = C.courseVersion
        LEFT JOIN unit_acdEnroll E
               ON C.contractId = E.contractId
        LEFT JOIN unit_acdGroup G
               ON E.groupId = G.groupId
              AND G.learningPeriodId = LP.learningPeriodId
        LEFT JOIN unit_acdcurriculum CM
               ON G.curriculumId = CM.curriculumId
        LEFT JOIN acdCurricularComponent CC
               ON CC.curricularcomponentid = CM.curricularcomponentid
              AND CC.curricularcomponentversion = CM.curricularcomponentversion
            WHERE LP.learningperiodid = $P{learningPeriodId}
              AND C.personid = $P{personId}
              AND LP.courseid = $P{courseId}
              AND LP.courseversion = $P{courseVersion}
              AND G.financecredits > 0]]>
		</queryString>
		<field name="curricularcomponentid" class="java.lang.String"/>
		<field name="classid" class="java.lang.String"/>
		<field name="name" class="java.lang.String"/>
		<field name="academiccredits" class="java.lang.Double"/>
		<field name="creditvalue" class="java.math.BigDecimal"/>
		<field name="price" class="java.lang.Double"/>
		<field name="parcels" class="java.lang.Integer"/>
		<variable name="totalcredits" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{academiccredits}]]></variableExpression>
		</variable>
		<variable name="totalprice" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{price}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["../../academic/reports/"]]></defaultValueExpression>
	</parameter>
	<parameter name="personId" class="java.lang.Integer"/>
	<parameter name="courseId" class="java.lang.String"/>
	<parameter name="courseVersion" class="java.lang.Integer"/>
	<parameter name="unitId" class="java.lang.Integer"/>
	<parameter name="learningPeriodId" class="java.lang.Integer"/>
	<parameter name="SAGU_PATH" class="java.lang.String" isForPrompting="false">
		<parameterDescription><![CDATA["../../basic/reports/"]]></parameterDescription>
		<defaultValueExpression><![CDATA[SAGU_PATH]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT C.personId,
                C.name AS personName,
                DOC.content AS rg,
                CASE WHEN DOC.organ IS NULL
                  THEN ''
                  ELSE DOC.organ END AS rgOrgan,
                H.periodId AS period,
                A.courseId || '/' || A.courseVersion || ' - ' || B.name AS courseName,
                TO_CHAR((SELECT MIN(E.dateenroll)
                           FROM unit_acdEnroll E
                     INNER JOIN unit_acdGroup SG
                             ON E.groupId = SG.groupId
                          WHERE E.contractid = A.contractid
                            AND SG.learningperiodid = H.learningperiodid ),
                        (SELECT value
                           FROM basconfig
                          WHERE parameter = 'MASK_DATE'
                            AND moduleconfig = 'BASIC'
                          LIMIT 1)) AS dateEnroll,
                getPersonName(A.guarantorId) as guarantor,
                CASE WHEN (SELECT COUNT(*)
                             FROM basLegalPerson
                            WHERE personId = A.guarantorId) > 0
                  THEN (SELECT cnpj
                          FROM basLegalPerson
                         WHERE personId = A.guarantorId)
                  ELSE J.content END as guarantorDocument,
                getPersonName(I.legalresponsableid) as legalResponsable
           FROM unit_acdContract A
     INNER JOIN unit_acdcourseoccurrence M
             ON A.courseId = M.courseId
            AND A.courseVersion = M.courseVersion
            AND A.unitId = M.unitId
            AND A.turnId = M.turnId
     INNER JOIN acdCourse B
             ON M.courseId = B.courseId
 LEFT JOIN ONLY basPhysicalPerson C
             ON A.personId = C.personId
      LEFT JOIN basdocument DOC
             ON DOC.personId = A.personId
            AND DOC.documenttypeid = (SELECT value::integer
                                        FROM basConfig
                                       WHERE moduleConfig = 'BASIC'
                                         AND parameter = 'DEFAULT_DOCUMENT_TYPE_ID_RG'
                                       LIMIT 1)
      LEFT JOIN unit_acdEnroll F
             ON A.contractId = F.contractId
      LEFT JOIN unit_acdGroup G
             ON F.groupId = G.groupId
      LEFT JOIN unit_acdlearningperiod H
             ON G.learningPeriodId = H.learningPeriodId
      LEFT JOIN bascompanyconf I
             ON A.guarantorId = I.personId
      LEFT JOIN basDocument J
             ON A.guarantorId = J.personId
            AND J.documentTypeId = (SELECT value
                                      FROM basConfig
                                     WHERE parameter='DEFAULT_DOCUMENT_TYPE_ID_CPF')::int
          WHERE ($P{personId} IS NULL OR C.personId = $P{personId})
            AND ($P{courseId} IS NULL OR A.courseId = $P{courseId})
            AND ($P{courseVersion} IS NULL OR A.courseVersion = $P{courseVersion})
            AND ($P{unitId} IS NULL OR A.unitId = $P{unitId})
            AND ($P{learningPeriodId} IS NULL OR H.learningperiodid = $P{learningPeriodId})]]>
	</queryString>
