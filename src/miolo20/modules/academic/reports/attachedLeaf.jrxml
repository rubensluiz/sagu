	<subDataset name="horarios">
		<parameter name="groupid" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="lines" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT * FROM (SELECT D.visitingprofessorid,
			 E.name as visitingprofessorname,
			 F.personid as professorid,
			 F.name as professorname,
			 to_char(D.date, 'dd/mm/yyyy' ) || ' - ' || G.beginhour as time,
			 trim(replace(D.description, chr(10) || chr(10), '')) as description
			 FROM unit_acdGroup A
		  INNER JOIN acdSchedule B
			  ON (A.groupId = B.groupId)
		  INNER JOIN acdScheduleProfessor C
			  ON (B.scheduleId = C.scheduleId)
		  INNER JOIN acdscheduleprofessorcontent D
			  ON (C.scheduleProfessorid = D.scheduleProfessorid)
		   LEFT JOIN basphysicalpersonprofessor E
			  ON (D.visitingprofessorid = E.personid)
		   LEFT JOIN basphysicalpersonprofessor F
			  ON (C.professorid = F.personid)
		   LEFT JOIN acdtime G
			  ON (D.timeid = G.timeid)
		   WHERE A.groupid = $P{groupid}::int
		   ORDER BY D.date, G.beginhour) Y
		   UNION ALL
		   SELECT  null,
  		   null,
		   null,
		   null,
		   null,
		   null
	   FROM (SELECT UNNEST(array_fill(0, ARRAY[$P{lines}::int]))) X;]]>
		</queryString>
		<field name="visitingprofessorid" class="java.lang.Integer"/>
		<field name="visitingprofessorname" class="java.lang.String"/>
		<field name="professorid" class="java.lang.Integer"/>
		<field name="professorname" class="java.lang.String"/>
		<field name="time" class="java.lang.String"/>
		<field name="description" class="java.lang.String"/>
	</subDataset>
	<parameter name="groupid" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="lines" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="beginDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="endDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT DISTINCT  B.groupId,
                                 F.curricularComponentId || ' - ' || F.name as disciplina,
                                 L.personId as professorid,
	           L.name as professorname,
                                 C.periodId as periodo,
                                 M.description as unidade,
                                 F.lessonNumberHours as cargaHoraria,
                                 round(C.minimumFrequency * F.lessonNumberHours / 100) as frequencia,
                                 I.room || ' - ' || I.building as sala,
                                 P.name as centro,
                                 B.classId as turma,
	           F.academiccredits as creditos
                           FROM  unit_acdGroup B
                     INNER JOIN  unit_acdlearningperiod C
                             ON  (B.learningPeriodId = C.learningPeriodId)
                     INNER JOIN  unit_acdcurriculum E
                             ON  (B.curriculumId = E.curriculumId)
                     INNER JOIN  acdCurricularComponent F
                             ON  (E.curricularComponentId = F.curricularComponentId AND
                                  E.curricularComponentVersion = F.curricularComponentVersion)
                     INNER JOIN  acdSchedule G
                             ON  (B.groupId = G.groupId)
                      LEFT JOIN  insPhysicalResource I
                             ON  (I.physicalResourceId = G.physicalResourceId AND
                                  I.physicalResourceVersion = G.physicalResourceVersion)
                     INNER JOIN  acdCourse J
                             ON  (J.courseId = E.courseId)
                     INNER JOIN  acdScheduleProfessor K
                             ON  (G.scheduleId = K.scheduleId)
 		 LEFT JOIN acdScheduleProfessorContent PC
		        ON (PC.scheduleprofessorid = K.scheduleprofessorid)
                     LEFT JOIN  basPhysicalPersonProfessor L
                             ON  (K.professorId = L.personId)
                     INNER JOIN  basUnit M
                             ON  (C.unitId = M.unitId)
                      LEFT JOIN  acdCenter P
                             ON  (P.centerId = J.centerId)
                          WHERE  B.groupId = $P{groupid}::int
		       AND CASE WHEN (char_length($P{beginDate}) > 0 AND char_length($P{endDate}) > 0) THEN PC.date BETWEEN datetodb($P{beginDate}) AND datetodb($P{endDate}) ELSE TRUE END;]]>
	</queryString>
