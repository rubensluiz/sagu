	<queryString>
		<![CDATA[SELECT A.personid,
	A.invoiceid,
	TO_CHAR(A.maturityDate,'DD/MM/YYYY') as maturityDate,
	B.name as personName,
	C.contractId,
	D.courseId,
	D.courseVersion,
	E.name as courseName,
	F.turnId,
	F.description as turnDescription,
    H.content as cpf,
    B.location||' '||COALESCE(B.number,'')||CASE WHEN L.name is not null THEN ' - '||L.name END as endereco,

    K.descricao as curso_pedagogico,

    (CASE WHEN K.descricao IS NOT NULL
     THEN
            K.descricao
     WHEN D.courseId IS NOT NULL
     THEN
        D.courseId || '/' || D.courseVersion || ' - ' || E.name || ' - ' || F.description
     ELSE '-'
     END
    ) AS curso,

    COALESCE((SELECT datetouser(MAX(entryDate))
       FROM finEntry
      WHERE invoiceId = A.invoiceid
        AND operationid in (SELECT paymentoperation FROM finDefaultOperations)), '-') AS returndate,
    TO_CHAR(now()::DATE, 'DD/MM/YYYY') AS visto

FROM finreceivableinvoice A
INNER JOIN ONLY basPhysicalPerson B
	ON B.personid = A.personId

INNER JOIN finEntry C
	ON A.invoiceid = C.invoiceid
         -- Obter somente o primeiro finEntry
         AND C.entryId = (SELECT MIN(_E.entryId) FROM finEntry _E WHERE _E.invoiceId = C.invoiceId)

LEFT JOIN acdContract D
	ON D.contractid = C.contractid
LEFT JOIN acdCourse E
	ON E.courseId = D.courseId
LEFT JOIN basTurn F
	ON F.turnId = D.turnId
LEFT JOIN basdocumenttype G
          ON G.name = 'CPF'
LEFT JOIN basdocument H
          ON H.personid = A.personid AND H.documenttypeid = G.documenttypeid
LEFT JOIN prctituloinscricao I
          ON I.invoiceid = A.invoiceid
LEFT JOIN acpinscricao J
          ON J.inscricaoid = I.inscricaoid
LEFT JOIN acpofertacurso K
          ON K.ofertacursoid = J.ofertacursoid
LEFT JOIN bascity L
          ON L.cityid = B.cityid

WHERE A.invoiceid IN ($P!{invoices})
ORDER BY A.maturitydate ASC]]>
	</queryString>
