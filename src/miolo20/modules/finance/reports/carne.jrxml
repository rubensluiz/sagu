	<subDataset name="LanÃ§amentos">
		<parameter name="invoiceid" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT A.entryId,
	A.operationId,
	B.description,
	ROUND(A.value, getParameter('BASIC', 'REAL_ROUND_VALUE')::int ) as value,
	to_char(A.entrydate, 'dd/mm/yyyy') as data
FROM finEntry A
INNER JOIN finOperation B
        ON (A.operationId = B.operationId)
WHERE invoiceid = $P{invoiceid}
ORDER BY CASE WHEN B.operationtypeid = 'D' THEN 1 ELSE 99 END, A.entrydate ASC]]>
		</queryString>
		<field name="entryid" class="java.lang.Integer"/>
		<field name="operationid" class="java.lang.Integer"/>
