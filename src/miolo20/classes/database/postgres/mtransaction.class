class PostgresTransaction extends MTransaction
{
    /**
     * Brief Description.
     * Complete Description.
     *
     * @returns (tipo) desc
     *
     */
    function _begintransaction()
    {
        $this->conn->_execute("begin transaction");
    }

    /**
     * Brief Description.
     * Complete Description.
     *
     * @returns (tipo) desc
     *
     */
    function _commit()
    {
        $this->conn->_execute("commit");
    }

    /**
     * Brief Description.
     * Complete Description.
     *
     * @returns (tipo) desc
     *
     */
    function _rollback()
    {
        $this->conn->_execute("rollback");
    }
}
