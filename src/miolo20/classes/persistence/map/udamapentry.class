<?php

class UDAMapEntry
{
    private $to;
    private $from;
    private $toAssociative;
    private $fromAssociative;

    public function __construct($from, $to, $fromAssociative = NULL, $toAssociative = NULL)
    {
        $this->to = $to;
        $this->from = $from;
        $this->fromAssociative = $fromAssociative;
        $this->toAssociative = $toAssociative;
    }

    public function getTo()
    {
        return $this->to;
    }

    public function getFrom()
    {
        return $this->from;
    }

    public function getToAssociative()
    {
        return $this->toAssociative;
    }

    public function getFromAssociative()
    {
        return $this->fromAssociative;
    }
}
?>
