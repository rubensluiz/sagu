<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="mensagensPorAssunto" language="groovy" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="periodid" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT A.assuntodecontatoid,
count(A.mensagemouvidoriaid) as quantidade, B.descricao, C.description
from rccmensagemouvidoria A
INNER JOIN rccassuntodecontato B
ON A.assuntodecontatoid = B.assuntodecontatoid
INNER JOIN acdlearningperiod C
ON C.periodid = $P{periodid}
WHERE A.datahora > C.begindate
AND A.datahora < C.enddate
group by A.assuntodecontatoid, B.descricao, C.description
order by quantidade asc;]]>
	</queryString>
	<field name="assuntodecontatoid" class="java.lang.Integer"/>
	<field name="quantidade" class="java.lang.Long"/>
	<field name="descricao" class="java.lang.String"/>
	<field name="description" class="java.lang.String"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band height="79" splitType="Stretch">
			<staticText>
				<reportElement x="0" y="0" width="555" height="79"/>
				<textElement>
					<font size="24" isBold="true"/>
				</textElement>
				<text><![CDATA[Relacionamento com o cliente]]></text>
			</staticText>
		</band>
	</title>
	<pageHeader>
		<band height="22" splitType="Stretch">
			<textField>
				<reportElement x="0" y="0" width="555" height="22"/>
				<textElement markup="none">
					<font size="18" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["Mensagens por Assunto:" + $F{description}]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<summary>
		<band height="219" splitType="Stretch">
			<pie3DChart>
				<chart>
					<reportElement mode="Transparent" x="0" y="0" width="555" height="219" forecolor="#000000" backcolor="#FFFFFF"/>
					<chartTitle/>
					<chartSubtitle/>
					<chartLegend/>
				</chart>
				<pieDataset>
					<keyExpression><![CDATA[$F{descricao}]]></keyExpression>
					<valueExpression><![CDATA[$F{quantidade}]]></valueExpression>
					<labelExpression><![CDATA[$F{quantidade}.toString()]]></labelExpression>
				</pieDataset>
				<pie3DPlot>
					<plot/>
					<itemLabel/>
				</pie3DPlot>
			</pie3DChart>
		</band>
	</summary>
</jasperReport>
