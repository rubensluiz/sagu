	<queryString>
		<![CDATA[SELECT D.personId, D.name, D.email, D.residentialphone, D.workphone, D.cellphone
                  FROM acdEnroll A
            INNER JOIN acdContract B
                    ON (A.contractId = B.contractId)
            INNER JOIN acdEnrollStatus C
                    ON (C.statusId = A.statusId)
       INNER JOIN ONLY basPhysicalPerson D
                    ON (B.personId = D.personId)
                 WHERE A.groupId = $P{groupid}
	    ORDER BY D.name]]>
